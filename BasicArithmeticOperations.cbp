#include <stdio.h>
#include <stdlib.h>

void main()
{
    int x,y,add,mul,div,sub;
    int num=1;
    printf("-------------------------------\n");
    printf("---------- CALCULATE ----------\n");
    printf("-------------------------------\n");
    do{
        printf("Expected Output %d\n", num);
        printf("Input any 2 integers: ", &x, &y);
        scanf("%d %d", &x, &y);
            add = x + y;
            mul = x * y;
            div = x / y;
            sub = x - y;
            num++;
        printf("ADD = %d\n", add);
        printf("MUL = %d\n", mul);
        printf("DIV = %d\n", div);
        printf("SUB = %d\n", sub);
        printf("-------------------------------\n");

    }while(num<=100);
    return 0;
}
